{
    "env": {
        "node": true,
        "es2021": true,
        "browser": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": "off",
        "indent": ["error", 4],
        "semi": [2, "never"],
        "no-unused-vars": 1,
        "no-useless-escape": 1
    }
}